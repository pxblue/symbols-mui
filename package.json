    "@babel/node": "7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@pxblue/prettier-config": "^1.0.2",
    "@pxblue/symbols": "^1.0.0",
